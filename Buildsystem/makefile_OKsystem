SHELL = /bin/sh
.SUFFIXES :

define last-element
$(word $(words $1),$1)
endef
srcdir := $(shell cd $(dir $(call last-element, $(MAKEFILE_LIST))); pwd)

export

include $(srcdir)/Transitional/Buildsystem/makefile_standardgoals

export

include $(srcdir)/Transitional/Buildsystem/makefile_recursive_noneincluded

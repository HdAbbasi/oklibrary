okltest_non_disjoint_pairs(f) := (
  assert(okltest_non_disjoint_pair(buildq([f], lambda([S], block([R:f(S)], if emptyp(R) then R else first(R))))) = true),
  assert(f([{1,2},{2,3},{3,4}]) = [[1,2],[2,3]]).
  assert(f([{1,2,3},{1,2},{2,3}]) = [[1,2],[1,3],[2,3]]),

okltest_ecomp(f) := (
  assert(f({},{}) = {}),
  assert(f({},{1}) = {}),
  assert(f({{}},{}) = {{}}),
  assert(f({{}},{1}) = {{1}}),
  assert(f({{1},{2}},{1,2,3}) = {{2,3},{1,3}}),

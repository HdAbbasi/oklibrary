okltest_disjoint_set_system_p(f) := (
  assert(f({}) = true),
  assert(f({{}}) = true),
  assert(f({{1},{},{2}}) = true),
  assert(f({{1}}) = true),
  assert(f({{1,2},{2,3}}) = false),
  assert(f({{1},{2},{},{3},{3,4}}) = false),

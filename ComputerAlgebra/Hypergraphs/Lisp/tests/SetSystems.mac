okltest_partitionp(f) := block(
  assert(f({},{}) = true),
  assert(f({{}},{}) = false),
  assert(f({{1},{}},{1}) = false),
  assert(f({{1}},{1}) = true),
  assert(f({{}},{1}) = false),
  assert(f({{2}},{1}) = false),
  assert(f({{2},{1}},{1}) = false),
  assert(f({{1},{2},{3}},{1,2,3}) = true),
  assert(f({{1},{2,3},{3,4}},{1,2,3,4}) = false),
  assert(f({{1,2,3,4}},{1,2,3,4}) = true),
  assert(f({{1},{2},{3},{4}},{1,2,3}) = false),
  true)$



okltest_setdifference2(f) := (
  assert(f({},{}) = {}),
  assert(f({{1},{}},{}) = {{1},{}}),
  assert(f({{1},{}},{2}) = {{1},{}}),
  assert(f({{1,2},{2,3}},{1}) = {{2},{2,3}}),
  assert(f({{1,2},{2,3}},{1,2,3}) = {{}}),
  assert(f([],{}) = []),
  assert(f([{1},{}],{}) = [{1},{}]),
  assert(f([{1},{}],{2}) = [{1},{}]),
  assert(f([{1,2},{2,3}],{1}) = [{2},{2,3}]),
  assert(f([{1,2},{2,3}],{1,2,3}) = [{},{}]),
  true)$

okltest_setdifference2e(f) := (
  assert(f([],1) = []),
  assert(f({},1) = {}),

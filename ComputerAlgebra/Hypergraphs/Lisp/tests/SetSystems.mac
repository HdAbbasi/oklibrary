okltest_non_disjoint_pair(f) := (
  assert(f([]) = []),
  assert(f([{}]) = []),
  assert(f([{1},{2}]) = []),
  assert(f([{1},{1,2}]) = [1,2]),
  assert(f([{1,2},{3,4},{5,6},{5,7}]) = [3,4]),

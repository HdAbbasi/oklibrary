okltest_g_p(f) := block(
  for G in okltest_list_g do
    assert(f(G) = true),
  assert(f([]) = false),
  assert(f([[],{}]) = false),
  assert(f([{},[]]) = false),
  assert(f([[],[]]) = false),
  assert(f([{},{{}}]) = false),
  assert(f([{},{{1,2}}]) = false),
  assert(f([{1},{{1}}]) = false),
  assert(f([{1,2},{[1,2]}]) = false),
  assert(f([{1,2,3},{{1,2,3}}]) = false),
  true)$


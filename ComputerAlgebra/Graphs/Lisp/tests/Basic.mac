  assert(f([{},{},k]) = [[],[],k]),
  assert(f([{1},{},k]) = [[1],[],k]),
  assert(f([{1,2,3},{1,2,3},lambda([e],if e=1 then [1,2] elseif e=2 then [2,3] else [3,1])]) = [[1,2,3],[1,2,3],lambda([e],if e=1 then [1,2] elseif e=2 then [2,3] else [3,1])]),
  for G in okltest_list_gg do
    assert(ogdg2gdg(f(G)) = G),

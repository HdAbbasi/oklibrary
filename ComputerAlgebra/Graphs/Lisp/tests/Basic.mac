  assert(gr_equalp(f([[1],[],k1]), [[1],[],k2]) = true),
  assert(gr_equalp(f([[2,1],[{1,2},{1}],lambda([e],2)]), [[2,1],[[{1,2},1],[{1,2},2],[{1},1],[{1},2]],first]) = true),
  for L in [okltest_list_omug,okltest_list_omugl] do
    for G in L do
      assert(gr_equalp(ogg2omugl(f(G)),G) = true),
  true)$


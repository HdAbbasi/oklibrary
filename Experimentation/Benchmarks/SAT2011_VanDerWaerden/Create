${vdw_gen} 3 34 1144 
${vdw_gen} 3 35 1205 
${vdw_gen} 3 36 1258 
${vdw_gen} 3 37 1339 
${vdw_gen} 3 38 1379 
${vdw_gen} 3 39 1419 

cd ..

md5sum --check --status MD5_vdw


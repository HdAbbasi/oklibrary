# Reads minisat output from standard input, printing the extracted numerical
# data to standard output.

set -o errexit
set -o nounset

script_name="ExtractMinisat"
version_number=0.0.5

# Extraction of path to source code:
src_path="${BASH_SOURCE[0]}";
while([ -h "${src_path}" ]) do src_path=$(readlink "${src_path}"); done
cd $(dirname ${src_path}) > /dev/null
src_path=$(pwd)
cd - > /dev/null

extract_awk=${src_path}/ExtractMinisat.awk

awk -f ${extract_awk}

exit 0
